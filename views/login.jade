extends layout
block page
	- var menu = "login"
block content
	.col-md-4.col-md-offset-4
		h2 Log in
		div= err
		div
			form(active="/login",method="POST")
				p
					input.form-control(name="email" type="email" placeholder="Email")
				p
					input.form-control(name="password" type="password" placeholder="Password")
				p
					button.btn.btn-primary.btn-block(type="submit") Log in
